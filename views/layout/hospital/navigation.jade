.navbar.navbar-default.navbar-static-top.m-b-0
        .navbar-header
            a.navbar-toggle.hidden-sm.hidden-md.hidden-lg(href="javascript:void(0)", data-toggle="collapse", data-target=".navbar-collapse")
                i.ti-menu

            .top-left-part
                a.logo(href="index.html")
                    b
                        img(src=baseDir+"/hospital/plugins/images/eliteadmin-logo.png", alt="home")
                    span.hidden-xs
                        strong elite
                        | hospital

            ul.nav.navbar-top-links.navbar-left.hidden-xs
                li
                    a.open-close.hidden-xs.waves-effect.waves-light(href="javascript:void(0)")
                        i.icon-arrow-left-circle.ti-menu

                li
                    form.app-search.hidden-xs(role="search")
                        input.form-control(type="text", placeholder="Search...")

                        a(href="")
                            i.fa.fa-search

            ul.nav.navbar-top-links.navbar-right.pull-right
                li.dropdown
                    a.dropdown-toggle.waves-effect.waves-light(data-toggle="dropdown", href="#")
                        i.icon-envelope

                        .notify
                            span.heartbit
                            span.point

                    ul.dropdown-menu.mailbox.animated.bounceInDown
                        li
                            .drop-title You have 4 new messages

                        li
                            .message-center
                                a(href="#")
                                    .user-img
                                        img.img-circle(src=baseDir+"/hospital/plugins/images/users/pawandeep.jpg", alt="user")

                                        span.profile-status.online.pull-right

                                    .mail-contnet
                                        h5 Pavan kumar

                                        span.mail-desc Just see the my admin!

                                        span.time 9:30 AM

                                a(href="#")
                                    .user-img
                                        img.img-circle(src=baseDir+"/hospital/plugins/images/users/sonu.jpg", alt="user")

                                        span.profile-status.busy.pull-right

                                    .mail-contnet
                                        h5 Sonu Nigam

                                        span.mail-desc I've sung a song! See you at

                                        span.time 9:10 AM

                                a(href="#")
                                    .user-img
                                        img.img-circle(src=baseDir+"/hospital/plugins/images/users/arijit.jpg", alt="user")

                                        span.profile-status.away.pull-right

                                    .mail-contnet
                                        h5 Arijit Sinh

                                        span.mail-desc I am a singer!

                                        span.time 9:08 AM

                                a(href="#")
                                    .user-img
                                        img.img-circle(src="plugins/images/users/pawandeep.jpg", alt="user")

                                        span.profile-status.offline.pull-right

                                    .mail-contnet
                                        h5 Pavan kumar

                                        span.mail-desc Just see the my admin!

                                        span.time 9:02 AM

                        li
                            a.text-center(href="javascript:void(0);")
                                strong See all notifications

                                i.fa.fa-angle-right
                    // /.dropdown-messages
                // /.dropdown
                li.dropdown
                    a.dropdown-toggle.waves-effect.waves-light(data-toggle="dropdown", href="#")
                        i.icon-note

                        .notify
                            span.heartbit
                            span.point

                    ul.dropdown-menu.dropdown-tasks.animated.slideInUp
                        li
                            a(href="#")
                                div
                                    p
                                        strong Task 1

                                        span.pull-right.text-muted 40% Complete

                                    .progress.progress-striped.active
                                        .progress-bar.progress-bar-success(role="progressbar", aria-valuenow="40", aria-valuemin="0", aria-valuemax="100", style="width: 40%")
                                            span.sr-only 40% Complete (success)

                        li.divider

                        li
                            a(href="#")
                                div
                                    p
                                        strong Task 2

                                        span.pull-right.text-muted 20% Complete

                                    .progress.progress-striped.active
                                        .progress-bar.progress-bar-info(role="progressbar", aria-valuenow="20", aria-valuemin="0", aria-valuemax="100", style="width: 20%")
                                            span.sr-only 20% Complete

                        li.divider

                        li
                            a(href="#")
                                div
                                    p
                                        strong Task 3

                                        span.pull-right.text-muted 60% Complete

                                    .progress.progress-striped.active
                                        .progress-bar.progress-bar-warning(role="progressbar", aria-valuenow="60", aria-valuemin="0", aria-valuemax="100", style="width: 60%")
                                            span.sr-only 60% Complete (warning)

                        li.divider

                        li
                            a(href="#")
                                div
                                    p
                                        strong Task 4

                                        span.pull-right.text-muted 80% Complete

                                    .progress.progress-striped.active
                                        .progress-bar.progress-bar-danger(role="progressbar", aria-valuenow="80", aria-valuemin="0", aria-valuemax="100", style="width: 80%")
                                            span.sr-only 80% Complete (danger)

                        li.divider

                        li
                            a.text-center(href="#")
                                strong See All Tasks

                                i.fa.fa-angle-right
                    // /.dropdown-tasks
                // /.dropdown
                li.dropdown
                    a.dropdown-toggle.profile-pic(data-toggle="dropdown", href="#")
                        img.img-circle(src=baseDir+"/hospital/plugins/images/users/d1.jpg", alt="user-img", width="36")
                        b.hidden-xs Dr. Steave

                    ul.dropdown-menu.dropdown-user.animated.flipInY
                        li
                            a(href="javascript:void(0)")
                                i.ti-user
                                |   My Profile

                        li
                            a(href="javascript:void(0)")
                                i.ti-email
                                |   Inbox

                        li
                            a(href="javascript:void(0)")
                                i.ti-settings
                                |   Account Setting

                        li
                            a(href="login.html")
                                i.fa.fa-power-off
                                |   Logout
                    // /.dropdown-user

                li.right-side-toggle
                    a.waves-effect.waves-light(href="javascript:void(0)")
                        i.ti-settings
                // /.dropdown
        // /.navbar-header
